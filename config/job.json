{
    "source_format": "CSV",
    "write_disposition": "WRITE_TRUNCATE",
    "ignore_unknown_values": true,
    "partitioning_type": "MONTH",
    "partitioning_field": "timestamp",
    "skip_leading_rows": 1,
    "schema": [
        {
            "name": "timestamp",
            "type": "DATE"
        },
        {
            "name": "open",
            "type": "FLOAT"
        },
        {
            "name": "high",
            "type": "FLOAT"
        },
        {
            "name": "low",
            "type": "FLOAT"
        },
        {
            "name": "close",
            "type": "FLOAT"
        },        
        {
            "name": "volume",
            "type": "FLOAT"
        },
        {
            "name": "symbol",
            "type": "STRING"
        }
    ]
}